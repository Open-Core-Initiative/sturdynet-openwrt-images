config wifi-device 'radio1'
	option channel 'auto'
	option country 'US'
	option disabled '0'
	option htmode 'VHT80'
	option hwmode '11a'
	option path 'platform/soc/a800000.wifi'
	option type 'mac80211'

config wifi-device 'radio0'
	option channel 'auto'
	option country 'US'
	option disabled '0'
	option htmode 'HT20'
	option hwmode '11g'
	option path 'platform/soc/a000000.wifi'
	option type 'mac80211'

config wifi-iface 'wifi_wlan0'
	option device 'radio0'
	option disabled '0'
	option encryption 'sae-mixed+ccmp'
	option ft_over_ds '1'
	option ft_psk_generate_local '0'
	option hidden '0'
	option ieee80211r '0'
	option ieee80211w '1'
	option ifname 'wlan0'
	option isolate '0'
	option key 'SturdyNet1!'
	option macfilter 'disable'
	option mode 'ap'
	option network 'lan'
	option reassociation_deadline '1000'
	option rsn_preauth '0'
	option ssid 'SturdyNet'
	option wds '0'
	option wmm '1'

config wifi-iface 'wifi_wlan1'
	option device 'radio1'
	option disabled '0'
	option encryption 'sae-mixed+ccmp'
	option ft_over_ds '1'
	option ft_psk_generate_local '0'
	option hidden '0'
	option ieee80211r '0'
	option ieee80211w '1'
	option ifname 'wlan1'
	option isolate '0'
	option key 'SturdyNet1!'
	option macfilter 'disable'
	option mode 'ap'
	option network 'lan'
	option reassociation_deadline '1000'
	option rsn_preauth '0'
	option ssid 'SturdyNet'
	option wds '0'
	option wmm '1'
