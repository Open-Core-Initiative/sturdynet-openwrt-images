config interface 'loopback'
	option device 'lo'
	option proto 'static'
	option ipaddr '127.0.0.1'
	option netmask '255.0.0.0'

config globals 'globals'
	option ula_prefix 'fd7c:cabe:56e7::/48'

config device 'device1'
	option name 'br-lan'
	list ports 'eth0'
	option type 'bridge'

config interface 'lan'
	option device 'br-lan'
	option proto 'static'
	option netmask '255.255.255.0'
	option ip6assign '60'
	option delegate '0'
	option ipaddr '192.168.8.1'

config switch 'switch0'
	option enable_vlan '1'
	option name 'switch0'
	option reset '1'

config switch_vlan 'switch0_vlan1'
	option device 'switch0'
	option ports '1 2 3 4 0'
	option vlan '1'

config switch_vlan 'switch0_vlan2'
	option device 'switch0'
	option ports '5 0'
	option vlan '2'

config interface 'lte6_4'
	option proto '464xlat'
	option ip6prefix '64:ff9b::/96'
	option metric '20'

config interface 'wan'
	option proto 'dhcp'
	option device 'eth1'
	option metric '10'

config route 'route1'
	option interface 'eth1'
	option metric '0'
	option netmask '255.255.255.255'
	option onlink '0'
	option target '192.168.100.1'
	option type 'unicast'

config interface 'lte'
	option proto 'modemmanager'
	option device '/sys/devices/platform/soc/8af8800.usb3/8a00000.dwc3/xhci-hcd.0.auto/usb1/1-1'
	option apn 'fast.t-mobile.com'
	option auth 'none'
	option iptype 'ipv4v6'
	option signalrate '5'
