config rule 'DNS'
	option family 'ipv4'
	option dest_port '53'
	option proto 'udp'
	option sticky '0'
	option use_policy 'Pri_LLN'

config rule 'RTTY'
	option family 'ipv4'
	option dest_ip '52.33.26.102'
	option dest_port '5912'
	option proto 'tcp'
	option sticky '0'
	option use_policy 'Pri_LLN'

config rule 'SAT_Modem'
	option family 'ipv4'
	option dest_ip '192.168.100.1'
	option sticky '0'
	option use_policy 'Pri_Sat'

config rule 'Viasat_VPN'
	option family 'ipv4'
	option dest_ip '8.37.96.0/20'
	option dest_port '443'
	option proto 'udp'
	option sticky '0'
	option use_policy 'Pri_LLN'

config rule 'STREAMING_MEDIA'
	option proto 'all'
	option sticky '0'
	option ipset 'NFA4_APPCAT_STREAMING_MEDIA'
	option use_policy 'Pri_Sat'
	option family 'ipv4'

config rule 'VOIP'
	option family 'ipv4'
	option proto 'all'
	option sticky '0'
	option ipset 'NFA4_APPCAT_VOIP'
	option use_policy 'Pri_LLN'

config rule 'VPN_AND_PROXY'
	option family 'ipv4'
	option proto 'all'
	option sticky '0'
	option ipset 'NFA4_APPCAT_VPN_AND_PROXY'
	option use_policy 'Pri_LLN'

config rule 'GAMES'
	option family 'ipv4'
	option proto 'all'
	option sticky '0'
	option ipset 'NFA4_APPCAT_GAMES'
	option use_policy 'Pri_LLN'

config rule 'REMOTE_DESKTOP'
	option family 'ipv4'
	option proto 'all'
	option sticky '0'
	option ipset 'NFA4_APPCAT_REMOTE_DESKTOP'
	option use_policy 'Pri_LLN'

config rule 'default'
	option family 'ipv4'
	option dest_ip '0.0.0.0/0'
	option proto 'all'
	option sticky '0'
	option use_policy 'Load_Balance'

config globals 'globals'
	option mmx_mask '0x3F00'

config interface 'wan'
	option initial_state 'online'
	option family 'ipv4'
	list track_ip '8.8.8.8'
	option track_method 'ping'
	option reliability '1'
	option count '3'
	option size '56'
	option max_ttl '60'
	option check_quality '0'
	option timeout '4'
	option interval '10'
	option failure_interval '5'
	option recovery_interval '5'
	option up '5'
	option enabled '1'
	option down '3'

config member 'wan_m1_w2'
	option interface 'wan'
	option metric '1'
	option weight '2'

config member 'wwan_m1_w2'
	option metric '1'
	option weight '2'
	option interface 'lte'

config policy 'Pri_Sat'
	list use_member 'wan_m1_w2'
	option last_resort 'default'

config policy 'Pri_LLN'
	list use_member 'wwan_m1_w2'
	option last_resort 'default'

config policy 'Load_Balance'
	list use_member 'wan_m1_w2'
	list use_member 'wwan_m1_w2'
	option last_resort 'unreachable'

config interface 'lte'
	option enabled '1'
	option initial_state 'online'
	option family 'ipv4'
	list track_ip '8.8.8.8'
	list track_ip '4.2.2.2'
	option track_method 'ping'
	option reliability '1'
	option count '1'
	option size '56'
	option max_ttl '60'
	option check_quality '0'
	option timeout '4'
	option interval '10'
	option failure_interval '5'
	option recovery_interval '5'
	option down '3'
	option up '3'
